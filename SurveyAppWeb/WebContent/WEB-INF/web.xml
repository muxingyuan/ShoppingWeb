<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>SurveyAppWeb</display-name>
  <welcome-file-list>
    <welcome-file>ChooseSurvey.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <description></description>
    <display-name>SurveyListServlet</display-name>
    <servlet-name>SurveyListServlet</servlet-name>
    <servlet-class>com.project.survey.SurveyListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SurveyListServlet</servlet-name>
    <url-pattern>/SurveyListServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <display-name>SurveyQuestionServlet</display-name>
    <servlet-name>SurveyQuestionServlet</servlet-name>
    <servlet-class>com.project.survey.SurveyQuestionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SurveyQuestionServlet</servlet-name>
    <url-pattern>/SurveyQuestionServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <description></description>
    <display-name>StatServlet</display-name>
    <servlet-name>StatServlet</servlet-name>
    <servlet-class>com.project.survey.StatServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StatServlet</servlet-name>
    <url-pattern>/StatServlet</url-pattern>
  </servlet-mapping>

</web-app>